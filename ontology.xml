    <!-- http://www.semanticweb.org/ontologies/2019/11/SIDRO#Artwork -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2019/11/SIDRO#Artwork">
        <rdfs:comment>Any physical artwork that can be observed. For instance a painting or a statue.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/2019/11/SIDRO#Character -->


    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2019/11/SIDRO#Character">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2019/11/SIDRO#Element"/>
        <rdfs:comment>Any element of the artwork&apos;s content that is a living being.</rdfs:comment>
    </owl:Class>
    


